<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nowrunning.travelrun"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.nowrunning.travelrun.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.nowrunning.travelrun.profileActivity" >
        </activity>
        <activity android:name="com.nowrunning.travelrun.otherProfileActivity" >
        </activity>
        <activity android:name="com.nowrunning.travelrun.tester" >
        </activity>
        <activity android:name="com.nowrunning.travelrun.ProfileTest" >
        </activity>
        <activity android:name="com.nowrunning.travelrun.ProfileActivity" >
        </activity>
        <activity android:name="com.nowrunning.travelrun.otherPersonProfile" >
        </activity>
    </application>

</manifest>